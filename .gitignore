# Specify only the .vim files that need to be versioned
vim.symdot/*
!vim.symdot/autoload/
!vim.symdot/ftplugin/
!vim.symdot/ftdetect/
!vim.symdot/plugins.vim
!vim.symdot/vimrc

# the same for nvim
nvim.symconfig/*
!nvim.symconfig/autoload/
!nvim.symconfig/ftplugin/
!nvim.symconfig/ftdetect/
!nvim.symconfig/plugins.vim
!nvim.symconfig/init.vim

# zsh
zsh/.*
zsh/cache/

# Local files
*.local*

# FZF
fzf/
